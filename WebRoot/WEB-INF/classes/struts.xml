<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE struts PUBLIC  
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"  
    "http://struts.apache.org/dtds/struts-2.3.dtd">


<struts>
	<package name="struts2" extends="struts-default">
		<action name="*_page" class="com.freefire.MainPage">
			<result>/WEB-INF/pages/${value}_page.jsp</result>
		</action>
		<action name="news" class="com.freefire.News">
			<result>/WEB-INF/pages/news_page.jsp</result>
		</action>
		<action name="members" class="com.freefire.Members">
			<result>/WEB-INF/pages/members_page.jsp</result>
		</action>
	</package>
</struts>